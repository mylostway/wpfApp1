{
  // 服务器地址
  "ServerHost": "localhost",
  // 服务器端口
  "ServerPort": 8907,
  // 是否单机测试模式（使用Fake数据源）
  "IsSingleTestMode": false,
  // 是否自动生成测试数据(发到后台测试，懒的按数据了)
  "IsCreateDataForTest": true,
  // 慢请求默认超时时间（毫秒）
  "DefaultRequestTimeout": 3000,
  // 默认客户端阻塞等待超时时间（毫秒）
  "DefaultClientWaitTimeout": 2000
}